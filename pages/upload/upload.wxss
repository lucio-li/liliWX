/* pages/upload/upload.wxss */
page {
  background: #f0f0f0;
}
.container {
  position: relative;
  padding:0;
}
.header {
  background: white;
  width: 100%;
}
.header textarea {
  width: 100%;
}
.chooseImageBtn {
  
  margin-top: 50rpx;
  margin-left: 25rpx;
  margin-bottom: 25rpx;
  width: 150rpx;
  height: 150rpx;
  display: block;
}
.chooseImages {
  display: inline-block;
  position: relative;
}
.chooseImagesList {
  position: relative;
  width: 130rpx;
  height: 130rpx;
  margin: 5rpx;
  z-index: 10;
}
.deleteImageBtn {
  width: 60rpx;
  height: 60rpx;
  position: absolute;
  left: 0rpx;
  top: 0rpx;
  z-index: 100;
}




.timeTip {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  margin: 20rpx 0;
  background: white;
}
.timeTipLeft {
  margin-left: 25rpx; 
}
.timeTipRight {
  float: right;
  margin-right: 25rpx; 
}
.locationTip {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  margin-bottom: 350rpx;
  background: white;
}
.locationTipLeft {
  margin-left: 25rpx; 
}
.locationTipRight {
  float: right;
  margin-right: 25rpx; 
}
.booter {
  background: white;
  position: fixed;
  bottom: 10rpx;
  width: 100%;
  padding-top: 15rpx; 
}
.booter button {
  width: 95%;
  height: 100rpx;
  background: #fe93b1;
  color: white;
}





